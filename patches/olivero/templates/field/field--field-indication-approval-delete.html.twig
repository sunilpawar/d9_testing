{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
  ]
%}
{%
  set title_classes = [
    'field__label',
    label_display == 'visually_hidden' ? 'visually-hidden',
  ]
%}

{% for item in items %}
  {% if item.content['#text'] != '12/31/1969' %}
    <div{{ attributes.addClass(classes, ['field__item']) }}>({% spaceless %}{{ item.content }}{% endspaceless %})</div>
  {% endif %}
{% endfor %}
