{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "behat/mink": "1.8.x-dev as 1.7.x-dev",
        "behat/mink-selenium2-driver": "1.4.x-dev as 1.3.x-dev",
        "composer/installers": "^1.9",
        "doctrine/annotations": "^1.12",
        "drupal/admin_theme": "^1.0@beta",
        "drupal/admin_toolbar": "^3.0",
        "drupal/allowed_formats": "^1.3",
        "drupal/antibot": "^1.4",
        "drupal/autologout": "^1.3",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bring_postal_code": "1.x-dev@dev",
        "drupal/cer": "4.x-dev@dev",
        "drupal/chosen": "^3.0",
        "drupal/components": "^2.4",
        "drupal/core": "9.2.6",
        "drupal/core-composer-scaffold": "9.2.6",
        "drupal/core-project-message": "9.2.6",
        "drupal/core-recommended": "9.2.6",
        "drupal/csv_serialization": "^2.0",
        "drupal/ctools": "^3.7",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/easy_social": "3.x-dev@dev",
        "drupal/field_display_label": "^1.2",
        "drupal/field_group": "^3.1",
        "drupal/field_permissions": "^1.1",
        "drupal/first_time_login": "^2.1",
        "drupal/formblock": "^2.0@beta",
        "drupal/honeypot": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "^5.0@beta",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_link_config": "^1.0@alpha",
        "drupal/node_view_permissions": "^1.5",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/search404": "^2.0",
        "drupal/search_autocomplete": "^2.0",
        "drupal/simple_popup_blocks": "^2.8",
        "drupal/token": "^1.9",
        "drupal/twig_tweak": "^3.1",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_data_export": "^1.0",
        "drush/drush": "^10.5",
        "guzzlehttp/guzzle": "^6.0@dev",
        "typo3/phar-stream-wrapper": "v3.1.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
